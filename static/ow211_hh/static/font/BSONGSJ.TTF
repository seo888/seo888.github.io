<script type="text/javascript">	
//首页弹窗广告
	function showLayer(imgurl,imglink){		
		//阻止冒泡
		function stopBubbling(e) {
			e = window.event || e;
			if (e.stopPropagation) {
				e.stopPropagation();      //阻止事件 冒泡传播
			} else {
				e.cancelBubble = true;   //ie兼容
			}
		}		
		let bodybox=document.querySelector('body');
		let showbox=document.createElement('div');
		let closespan=document.createElement('span');		
		// var s1=document.createElement('span');
		let s=33;
		closespan.style.cssText='position:absolute; font-size: 14px; color: #FFFFFF; right:5px; top:5px; z-index: 34;';
		closespan.innerHTML=(s-3)+'秒后关闭';
		// s1.style.cssText='position:absolute; font-size: 14px; color: #000000; left:5px; top:5px; z-index: 34;';		
		// s1.innerHTML='还有10秒后关闭';		
		showboxstyle='position:fixed; margin:auto;left:0;right:0;bottom:30px; height: 120px;width:1000px; background: url('+arguments[0]+');cursor:pointer;z-index: 33;'
		showboxsnone='display: none;';
		showboxsblock='display: block;';		
		showbox.style.cssText=showboxstyle+showboxsnone;		
		// showbox.appendChild(s1);
		showbox.appendChild(closespan);
		bodybox.appendChild(showbox);		
		if(arguments[1]){
			let address=arguments[1];
			console.log(address);
			showbox.addEventListener('click',function(){
				let tempwindow=window.open('_blank');
				tempwindow.location=address;
				// tempwindow.location='javascript:void()';
				stopBubbling(event); 
			},false);
		};			
		var inter = setInterval(function(){
			s--;
			closespan.innerHTML = s+'秒后关闭';
			if(s <= 0) {							
				clearInterval(inter);
			};
		}, 1000);			
		mytiem=setTimeout(function(){
			showbox.style.cssText=showboxstyle+showboxsblock;
			setTimeout(function(){
				showbox.style.cssText=showboxstyle+showboxsnone;
				clearTimeout(mytiem);					
			},30000);
		}, 3000);						
		closespan.addEventListener('click',function HiddenLayer(){
			clearTimeout(mytiem);		
			showbox.style.cssText=showboxstyle+showboxsnone;		 
		    stopBubbling(event); 	
		});			
	};	
	// addEventListener兼容ie
	function addListener (element, event, fn) {
		if (window.attachEvent) {
			element.attachEvent('on' + event, fn);
		} 
		else {
			element.addEventListener(event, fn, false);
		}
	}	
